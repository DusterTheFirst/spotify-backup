{
    "$schema": "https://json.schemastore.org/web-manifest-combined.json",
    "background_color": "#121212",
    "theme_color": "#121212",
    "name": "Spotify backup control panel",
    "short_name": "Spotify backup",
    "description": "Web panel to configure spotify backup",
    "display": "standalone",
    "scope": "/",
    "start_url": "/",
    "lang": "en-US",
    "orientation": "portrait-primary",
    "icons": [
        {
            "sizes": "1024x1024",
            "type": "image/png",
            "src": "/assets/spotify_1024.png"
        },
        {
            "sizes": "512x512",
            "type": "image/png",
            "src": "/assets/spotify_512.png"
        },
        {
            "sizes": "192x192",
            "type": "image/png",
            "src": "/assets/spotify_192.png"
        }
    ],
    "shortcuts": [
        {
            "name": "Log Out",
            "description": "Log out of the system",
            "url": "/de-auth"
        }
    ]
}