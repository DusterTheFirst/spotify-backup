{
    "$schema": "https://json.schemastore.org/web-manifest-combined.json",

    "short_name": "Spotify Backup",
    "name": "Spotify Backup",
    "description": "Utility to backup spotify playlists to a github repository",
    "lang": "en-US",

    "display": "standalone",
    "orientation": "portrait-primary",

    "scope": "/",
    "start_url": "/?ref=pwa",
    "id": "spotify-backup",

    "icons": [
        {
            "sizes": "1024x1024",
            "type": "image/webp",
            "src": "/static/branding/logo@1024.webp"
        },
        {
            "sizes": "1024x1024",
            "type": "image/png",
            "src": "/static/branding/logo@1024.png"
        },
        {
            "sizes": "512x512",
            "type": "image/webp",
            "src": "/static/branding/logo@512.webp"
        },
        {
            "sizes": "512x512",
            "type": "image/png",
            "src": "/static/branding/logo@512.png"
        },
        {
            "sizes": "192x192",
            "type": "image/webp",
            "src": "/static/branding/logo@192.webp"
        },
        {
            "sizes": "192x192",
            "type": "image/png",
            "src": "/static/branding/logo@192.png"
        }
    ]
}